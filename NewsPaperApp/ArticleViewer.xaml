<Window x:Name="articleviewer_window" x:Class="NewsPaperApp.ArticleViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewsPaperApp"
        mc:Ignorable="d"
        Title="ArticleViewer" Height="450" Width="800" Closed="articleviewer_window_Closed" Background="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}">
    <DockPanel LastChildFill="False">
        <StackPanel x:Name="stckpanel_left" Width="110" Background="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}"/>
        <ListBox Width="689" Background="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}"/>
    </DockPanel>
</Window>
